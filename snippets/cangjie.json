{
  "main": {
    "prefix": "main",
    "body": [
      "main() {",
      "\t$0",
      "}"
    ],
    "description": "Main function entry point"
  },
  "func": {
    "prefix": "func",
    "body": [
      "func ${1:name}(${2:params}): ${3:ReturnType} {",
      "\t$0",
      "}"
    ],
    "description": "Function definition"
  },
  "class": {
    "prefix": "class",
    "body": [
      "class ${1:ClassName} {",
      "\t$0",
      "}"
    ],
    "description": "Class definition"
  },
  "class-extends": {
    "prefix": "classe",
    "body": [
      "class ${1:ClassName} <: ${2:ParentClass} {",
      "\t$0",
      "}"
    ],
    "description": "Class with inheritance"
  },
  "struct": {
    "prefix": "struct",
    "body": [
      "struct ${1:StructName} {",
      "\t$0",
      "}"
    ],
    "description": "Struct definition"
  },
  "interface": {
    "prefix": "interface",
    "body": [
      "interface ${1:InterfaceName} {",
      "\t$0",
      "}"
    ],
    "description": "Interface definition"
  },
  "enum": {
    "prefix": "enum",
    "body": [
      "enum ${1:EnumName} {",
      "\t| ${2:Variant1}",
      "\t| ${3:Variant2}",
      "}"
    ],
    "description": "Enum definition"
  },
  "if": {
    "prefix": "if",
    "body": [
      "if (${1:condition}) {",
      "\t$0",
      "}"
    ],
    "description": "If statement"
  },
  "ifelse": {
    "prefix": "ife",
    "body": [
      "if (${1:condition}) {",
      "\t$2",
      "} else {",
      "\t$0",
      "}"
    ],
    "description": "If-else statement"
  },
  "match": {
    "prefix": "match",
    "body": [
      "match (${1:expr}) {",
      "\tcase ${2:pattern} => $0",
      "}"
    ],
    "description": "Match expression"
  },
  "for": {
    "prefix": "for",
    "body": [
      "for (${1:item} in ${2:collection}) {",
      "\t$0",
      "}"
    ],
    "description": "For loop"
  },
  "forrange": {
    "prefix": "forr",
    "body": [
      "for (${1:i} in ${2:0}..${3:10}) {",
      "\t$0",
      "}"
    ],
    "description": "For loop with range"
  },
  "while": {
    "prefix": "while",
    "body": [
      "while (${1:condition}) {",
      "\t$0",
      "}"
    ],
    "description": "While loop"
  },
  "try": {
    "prefix": "try",
    "body": [
      "try {",
      "\t$1",
      "} catch (e: ${2:Exception}) {",
      "\t$0",
      "}"
    ],
    "description": "Try-catch block"
  },
  "tryfinally": {
    "prefix": "tryf",
    "body": [
      "try {",
      "\t$1",
      "} catch (e: ${2:Exception}) {",
      "\t$3",
      "} finally {",
      "\t$0",
      "}"
    ],
    "description": "Try-catch-finally block"
  },
  "prop": {
    "prefix": "prop",
    "body": [
      "prop ${1:name}: ${2:Type} {",
      "\tget() { ${3:field} }",
      "\tset(value) { ${3:field} = value }",
      "}"
    ],
    "description": "Property with getter and setter"
  },
  "propget": {
    "prefix": "propg",
    "body": [
      "prop ${1:name}: ${2:Type} {",
      "\tget() { $0 }",
      "}"
    ],
    "description": "Read-only property"
  },
  "init": {
    "prefix": "init",
    "body": [
      "init(${1:params}) {",
      "\t$0",
      "}"
    ],
    "description": "Constructor/initializer"
  },
  "let": {
    "prefix": "let",
    "body": "let ${1:name}: ${2:Type} = ${0:value}",
    "description": "Immutable variable declaration"
  },
  "var": {
    "prefix": "var",
    "body": "var ${1:name}: ${2:Type} = ${0:value}",
    "description": "Mutable variable declaration"
  },
  "lambda": {
    "prefix": "lam",
    "body": "{ ${1:params} => $0 }",
    "description": "Lambda expression"
  },
  "spawn": {
    "prefix": "spawn",
    "body": [
      "spawn {",
      "\t$0",
      "}"
    ],
    "description": "Spawn concurrent task"
  },
  "synchronized": {
    "prefix": "sync",
    "body": [
      "synchronized(${1:lock}) {",
      "\t$0",
      "}"
    ],
    "description": "Synchronized block"
  },
  "package": {
    "prefix": "pkg",
    "body": "package ${1:name}",
    "description": "Package declaration"
  },
  "import": {
    "prefix": "imp",
    "body": "import ${1:package}.${0:*}",
    "description": "Import statement"
  },
  "importfrom": {
    "prefix": "impf",
    "body": "from ${1:package} import ${0:item}",
    "description": "From-import statement"
  },
  "println": {
    "prefix": "pl",
    "body": "println(${0:\"message\"})",
    "description": "Print line"
  },
  "print": {
    "prefix": "pr",
    "body": "print(${0:\"message\"})",
    "description": "Print without newline"
  },
  "test": {
    "prefix": "test",
    "body": [
      "@Test",
      "func test${1:Name}() {",
      "\t$0",
      "}"
    ],
    "description": "Test function"
  },
  "todo": {
    "prefix": "todo",
    "body": "// TODO: $0",
    "description": "TODO comment"
  },
  "fixme": {
    "prefix": "fixme",
    "body": "// FIXME: $0",
    "description": "FIXME comment"
  }
}
